{
    "name": "PlexMediaServer",
    "id": "plexmediaserver",
    "packaging_format": 1,
    "description": {
        "en": "PlexMediaServer package for YunoHost.",
        "fr": "PlexMediaServer pour YunoHost."
    },
    "version": "1.19.3.2843~ynh1",
    "url": "http://plex.tv/",
    "license": "non-free",
    "maintainer": {
        "name": "liberodark",
        "email": "liberodark@gmail.com"
    },
    "requirements": {
        "yunohost": ">= 4.2.4"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [{
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/web",
                "default": "/web"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
